#include<iostream>
#include<vector>
#include<map>
using namespace std;
int main(){
    //using another array
   vector<int>arr={1,3,8,2,9,7,6,5};
   int n=arr.size();
   int a[n]={0};
   for(int i=0;i<n;i++){
       a[arr[i]]++;
   }
   //thorugh hashing using map
    map<int,int>mp;
    for(int i=0;i<n;i++){
        mp[arr[i]]++;
    }
   
   
    }
    //print accordingly
