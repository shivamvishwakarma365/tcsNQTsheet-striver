//smallest and the largest both 
#include<iostream>

using namespace std;
int main(){
   vector<int>arr={1,3,8,2,9,7,6,5};
   int n=arr.size();
   int min=arr[0];
   int max=arr[0];
   for(int i=0;i<n;i++){
       if(arr[i]<min){
           min=arr[i];
       }else if(arr[i]>max){
           max=arr[i];
       }
   }
   cout<<min<<" "<<max;
   
   
    }
